{"ast":null,"code":"import ListItem from './components/ListItem.vue';\nimport Calculator from './components/Calculator.vue';\nexport default {\n  name: 'App',\n\n  data() {\n    return {\n      count: 0,\n      name: 'greek',\n      userInfo: {\n        'name': 'guest1',\n        'grade': 'supervisor'\n      },\n      arrayVue: [{\n        'number': '1',\n        'name': 'john',\n        'subject': 'math',\n        'level': '10'\n      }, {\n        'number': '2',\n        'name': 'tom',\n        'subject': 'english',\n        'level': '5'\n      }, {\n        'number': '3',\n        'name': 'amy',\n        'subject': 'english',\n        'level': '3'\n      }, {\n        'number': '4',\n        'name': 'testboy',\n        'subject': 'korean',\n        'level': '2'\n      }],\n      menus: ['menu1', 'menu2', 'menu3', 'menu4', 'menu5'],\n      num1: 0,\n      num2: 0,\n      result: 0\n    };\n  },\n\n  components: {\n    ListItem,\n    Calculator\n  },\n  methods: {\n    plus() {\n      this.count += 1;\n      console.log(this.count);\n    },\n\n    minus() {\n      let countValue = this.count; //let testA = document.querySelector('.count_num');\n\n      if (countValue == 0) {\n        alert('0 이하로는 내릴 수 없습니다.');\n        document.querySelector('.count_num').innerHTML = `<b style='color:#f00;'>${countValue}</b>`;\n      } else {\n        this.count -= 1;\n        console.log(this.count);\n      }\n    },\n\n    calcFunc(e) {\n      let num1 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num1]\").value);\n      let num2 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num2]\").value);\n      let num3 = '';\n      let result = document.querySelector(\".result_box h5 .num\");\n      let typeVal = e.target.innerText;\n\n      switch (typeVal) {\n        case \"+\":\n          num3 = num1 + num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        case \"-\":\n          num3 = num1 - num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        case \"*\":\n          num3 = num1 * num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        case \"/\":\n          num3 = num1 / num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        default:\n          alert(\"잘못된 계산식입니다. 내용을 확인해주세요.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA4BA,OAAOA,QAAP,MAAqB,2BAArB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,KAAI,EAAI,CADH;MAELF,IAAG,EAAI,OAFF;MAGLG,QAAO,EAAI;QACT,QAAS,QADA;QAET,SAAU;MAFD,CAHN;MAOLC,QAAO,EAAI,CAAC;QACV,UAAW,GADD;QAEV,QAAS,MAFC;QAGV,WAAY,MAHF;QAIV,SAAQ;MAJE,CAAD,EAKT;QACA,UAAW,GADX;QAEA,QAAS,KAFT;QAGA,WAAY,SAHZ;QAIA,SAAQ;MAJR,CALS,EAUT;QACA,UAAW,GADX;QAEA,QAAS,KAFT;QAGA,WAAY,SAHZ;QAIA,SAAQ;MAJR,CAVS,EAeT;QACA,UAAW,GADX;QAEA,QAAS,SAFT;QAGA,WAAY,QAHZ;QAIA,SAAU;MAJV,CAfS,CAPN;MA4BLC,KAAI,EAAI,CAAC,OAAD,EAAS,OAAT,EAAiB,OAAjB,EAAyB,OAAzB,EAAiC,OAAjC,CA5BH;MA6BLC,IAAG,EAAI,CA7BF;MA8BLC,IAAG,EAAI,CA9BF;MA+BLC,MAAK,EAAI;IA/BJ,CAAP;EAiCD,CApCY;;EAqCbC,UAAU,EAAE;IACVX,QADU;IAEVC;EAFU,CArCC;EAyCbW,OAAM,EAAI;IACRC,IAAI,GAAG;MACL,KAAKT,KAAL,IAAc,CAAd;MACAU,OAAO,CAACC,GAAR,CAAY,KAAKX,KAAjB;IAED,CALO;;IAMRY,KAAK,GAAG;MACN,IAAIC,UAAS,GAAI,KAAKb,KAAtB,CADM,CAEN;;MACA,IAAGa,UAAS,IAAK,CAAjB,EAAoB;QAClBC,KAAK,CAAC,mBAAD,CAAL;QACAC,QAAQ,CAACC,aAAT,CAAuB,YAAvB,EAAqCC,SAArC,GAAkD,0BAAyBJ,UAAW,MAAtF;MACF,CAHA,MAGO;QACL,KAAKb,KAAL,IAAc,CAAd;QACAU,OAAO,CAACC,GAAR,CAAY,KAAKX,KAAjB;MACF;IACD,CAhBO;;IAiBRkB,QAAQ,CAACC,CAAD,EAAI;MACV,IAAIf,IAAG,GAAIgB,QAAQ,CAACL,QAAQ,CAACC,aAAT,CAAuB,wCAAvB,EAAiEK,KAAlE,CAAnB;MACA,IAAIhB,IAAG,GAAIe,QAAQ,CAACL,QAAQ,CAACC,aAAT,CAAuB,wCAAvB,EAAiEK,KAAlE,CAAnB;MACA,IAAIC,IAAG,GAAI,EAAX;MACA,IAAIhB,MAAK,GAAIS,QAAQ,CAACC,aAAT,CAAuB,qBAAvB,CAAb;MACA,IAAIO,OAAM,GAAIJ,CAAC,CAACK,MAAF,CAASC,SAAvB;;MAGA,QAAOF,OAAP;QACE,KAAK,GAAL;UACED,IAAG,GAAIlB,IAAG,GAAIC,IAAd;UACAC,MAAM,CAACW,SAAP,GAAmBG,QAAQ,CAACE,IAAD,CAA3B;UACA;;QACF,KAAK,GAAL;UACEA,IAAG,GAAIlB,IAAG,GAAIC,IAAd;UACAC,MAAM,CAACW,SAAP,GAAmBG,QAAQ,CAACE,IAAD,CAA3B;UACA;;QACF,KAAK,GAAL;UACEA,IAAG,GAAIlB,IAAG,GAAIC,IAAd;UACAC,MAAM,CAACW,SAAP,GAAmBG,QAAQ,CAACE,IAAD,CAA3B;UACA;;QACF,KAAK,GAAL;UACEA,IAAG,GAAIlB,IAAG,GAAIC,IAAd;UACAC,MAAM,CAACW,SAAP,GAAmBG,QAAQ,CAACE,IAAD,CAA3B;UACA;;QACF;UACER,KAAK,CAAC,yBAAD,CAAL;MAlBJ;IAqBF;;EA9CQ;AAzCG,CAAf","names":["ListItem","Calculator","name","data","count","userInfo","arrayVue","menus","num1","num2","result","components","methods","plus","console","log","minus","countValue","alert","document","querySelector","innerHTML","calcFunc","e","parseInt","value","num3","typeVal","target","innerText"],"sourceRoot":"","sources":["C:\\dev\\vue\\test\\src\\App.vue"],"sourcesContent":["<template>\n    <div class=\"header\">\n        <ul>\n            <li v-for=\"menu in menus\" :key=\"menu\"><a href=\"#\">{{menu}}</a></li>\n        </ul>\n    </div>\n    <div class=\"box_count\">\n        \n        <h1><strong>*</strong> count Number 체크!</h1>\n        <p class=\"blind\">테스트 웹표준 blind 처리</p>\n        <h2 class=\"count_num\">{{ count }}</h2>\n        <button @click=\"plus\">+</button>\n        <button @click=\"minus\">-</button>\n\n        <Calculator></Calculator>\n\n        <!-- component 사용 -->\n        <ListItem :testProp=\"arrayVue\" :user=\"userInfo\"></ListItem>\n\n        <!-- 이미지 -->\n        <!-- <div class=\"img_wrap\">\n          <img src=\"./assets/logo.png\" />\n        </div> -->\n\n    </div> \n</template>\n\n<script>\nimport ListItem from './components/ListItem.vue';\nimport Calculator from './components/Calculator.vue';\n\nexport default {\n  name: 'App',\n  data() {\n    return {\n      count : 0,\n      name : 'greek',\n      userInfo : {\n        'name' : 'guest1',\n        'grade' : 'supervisor'\n      },\n      arrayVue : [{\n        'number' : '1',\n        'name' : 'john',\n        'subject' : 'math',\n        'level':'10'\n      },{\n        'number' : '2',\n        'name' : 'tom',\n        'subject' : 'english',\n        'level':'5'\n      },{\n        'number' : '3',\n        'name' : 'amy',\n        'subject' : 'english',\n        'level':'3'\n      },{\n        'number' : '4',\n        'name' : 'testboy',\n        'subject' : 'korean',\n        'level' : '2'\n      }],\n      menus : ['menu1','menu2','menu3','menu4','menu5'],\n      num1 : 0,\n      num2 : 0,\n      result : 0\n    }\n  },\n  components: {\n    ListItem,\n    Calculator\n  },\n  methods : {\n    plus() {\n      this.count += 1;\n      console.log(this.count);\n\n    },\n    minus() {\n      let countValue = this.count;\n      //let testA = document.querySelector('.count_num');\n      if(countValue == 0 ){\n        alert('0 이하로는 내릴 수 없습니다.');\n        document.querySelector('.count_num').innerHTML = `<b style='color:#f00;'>${countValue}</b>`;\n      } else {\n        this.count -= 1;\n        console.log(this.count);\n      }\n    },\n    calcFunc(e) {\n      let num1 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num1]\").value);\n      let num2 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num2]\").value);\n      let num3 = '';\n      let result = document.querySelector(\".result_box h5 .num\");\n      let typeVal = e.target.innerText;\n\n    \n      switch(typeVal) {\n        case \"+\" :          \n          num3 = num1 + num2;\n          result.innerHTML = parseInt(num3);\n          break;\n        case \"-\" :\n          num3 = num1 - num2;\n          result.innerHTML = parseInt(num3);\n          break;\n        case \"*\" :\n          num3 = num1 * num2;\n          result.innerHTML = parseInt(num3);\n          break;\n        case \"/\" :\n          num3 = num1 / num2;\n          result.innerHTML = parseInt(num3);\n          break;\n        default :\n          alert(\"잘못된 계산식입니다. 내용을 확인해주세요.\");\n      }\n      \n    }\n\n  }  \n}\n\n</script>\n\n<style lang=\"scss\">\n\n/* common.css */\n//폰트 컬러\n.fc-blue {color: #00f;}\n.blind {position: absolute;overflow: hidden; width: 1px; height: 1px; margin: -1px; clip:rect(0,0,0,0);}\nul { list-style: none; padding:0; }\na {text-decoration: 0; color: #2c3e50;}\n\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n  max-width: 550px;\n  margin:0 auto;\n}\n\n.header ul {\n  display:flex; justify-content: space-around;\n  li {\n    background: skyblue; padding:10px 12px;\n    a {\n      color:#fff; \n    }\n  }\n}\n\n@media screen and (max-width: 383px)  {\n    .header ul li {border:1px solid #000; padding:10px 5px;}\n}\n\n.calcul_box {\n  \n  .form_box {\n    display: flex;  \n    margin-top: 20px;\n    input {\n      width:calc(50% - 10px);\n      margin: 0 5px;\n    }\n  }\n}\n.box_count  {\n  button {\n  margin: 0 5px;\n  background: #2c3e50;\n  color: #fff;\n  border: 0;\n  padding:12px 18px;\n  border-radius: 10px;\n  font-size: 16px;\n  cursor: pointer;\n  }\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module"}