{"ast":null,"code":"export default {\n  name: 'calculator',\n\n  data() {},\n\n  props: {},\n  methods: {\n    calcFunc(e) {\n      let num1 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num1]\").value);\n      let num2 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num2]\").value);\n      let num3 = '';\n      let result = document.querySelector(\".result_box h5 .num\");\n      let typeVal = e.target.innerText;\n\n      switch (typeVal) {\n        case \"+\":\n          num3 = num1 + num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        case \"-\":\n          num3 = num1 - num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        case \"*\":\n          num3 = num1 * num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        case \"/\":\n          num3 = num1 / num2;\n          result.innerHTML = parseInt(num3);\n          break;\n\n        default:\n          alert(\"잘못된 계산식입니다. 내용을 확인해주세요.\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqBA,eAAe;EACXA,IAAG,EAAI,YADI;;EAEXC,IAAI,GAAG,CAEN,CAJU;;EAKXC,KAAK,EAAE,EALI;EAQXC,OAAM,EAAI;IACNC,QAAQ,CAACC,CAAD,EAAI;MACR,IAAIC,IAAG,GAAIC,QAAQ,CAACC,QAAQ,CAACC,aAAT,CAAuB,wCAAvB,EAAiEC,KAAlE,CAAnB;MACA,IAAIC,IAAG,GAAIJ,QAAQ,CAACC,QAAQ,CAACC,aAAT,CAAuB,wCAAvB,EAAiEC,KAAlE,CAAnB;MACA,IAAIE,IAAG,GAAI,EAAX;MACA,IAAIC,MAAK,GAAIL,QAAQ,CAACC,aAAT,CAAuB,qBAAvB,CAAb;MACA,IAAIK,OAAM,GAAIT,CAAC,CAACU,MAAF,CAASC,SAAvB;;MAGA,QAAOF,OAAP;QACI,KAAK,GAAL;UACAF,IAAG,GAAIN,IAAG,GAAIK,IAAd;UACAE,MAAM,CAACI,SAAP,GAAmBV,QAAQ,CAACK,IAAD,CAA3B;UACA;;QACA,KAAK,GAAL;UACAA,IAAG,GAAIN,IAAG,GAAIK,IAAd;UACAE,MAAM,CAACI,SAAP,GAAmBV,QAAQ,CAACK,IAAD,CAA3B;UACA;;QACA,KAAK,GAAL;UACAA,IAAG,GAAIN,IAAG,GAAIK,IAAd;UACAE,MAAM,CAACI,SAAP,GAAmBV,QAAQ,CAACK,IAAD,CAA3B;UACA;;QACA,KAAK,GAAL;UACAA,IAAG,GAAIN,IAAG,GAAIK,IAAd;UACAE,MAAM,CAACI,SAAP,GAAmBV,QAAQ,CAACK,IAAD,CAA3B;UACA;;QACA;UACAM,KAAK,CAAC,yBAAD,CAAL;MAlBJ;IAqBJ;;EA9BM;AARC,CAAf