ame=\"num2\" id=\"\">\r\n        </div>                      \r\n\r\n        <div class=\"result_box\">\r\n        <h5>결과값<span class=\"num\"></span></h5>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name : 'calculator-templates',\r\n    data() {\r\n\r\n    },\r\n    props: {\r\n\r\n    },\r\n    methods : {\r\n        calcFunc(e) {\r\n            let num1 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num1]\").value);\r\n            let num2 = parseInt(document.querySelector(\".calcul_box .form_box input[name=num2]\").value);\r\n            let num3 = '';\r\n            let result = document.querySelector(\".result_box h5 .num\");\r\n            let typeVal = e.target.innerText;\r\n\r\n            if((num1 != 0 && num2 != 0) && (!num1 || !num2)) {\r\n                alert(\"계산할 값을 각각 입력해주세요.\")                \r\n            } else {\r\n                switch(typeVal) {\r\n                    case \"+\" :          \r\n                        num3 = num1 + num2;\r\n                        result.innerHTML = parseInt(num3);\r\n                        break;\r\n                    case \"-\" :\r\n                        num3 = num1 - num2;\r\n                        result.innerHTML = parseInt(num3);\r\n                        break;\r\n                    case \"*\" :\r\n                        num3 = num1 * num2;\r\n                        result.innerHTML = parseInt(num3);\r\n                        break;\r\n                    case \"/\" :\r\n                        num3 = num1 / num2;\r\n                        result.innerHTML = parseInt(num3);\r\n                        break;\r\n                    default :\r\n                        alert(\"잘못된 계산식입니다. 내용을 확인해주세요.\");\r\n                }\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.calcul_box {\r\n    border: 1px solid #222;\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n}\r\n</style>"]}]}