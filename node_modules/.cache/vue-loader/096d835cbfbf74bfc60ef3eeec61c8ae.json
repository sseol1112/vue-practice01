 case \"+\" :          \r\n                num3 = num1 + num2;\r\n                result.innerHTML = parseInt(num3);\r\n                break;\r\n                case \"-\" :\r\n                num3 = num1 - num2;\r\n                result.innerHTML = parseInt(num3);\r\n                break;\r\n                case \"*\" :\r\n                num3 = num1 * num2;\r\n                result.innerHTML = parseInt(num3);\r\n                break;\r\n                case \"/\" :\r\n                num3 = num1 / num2;\r\n                result.innerHTML = parseInt(num3);\r\n                break;\r\n                default :\r\n                alert(\"잘못된 계산식입니다. 내용을 확인해주세요.\");\r\n            }\r\n            \r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.calcul_box {\r\n    border: 1px solid #222;\r\n    margin: 10px 0;\r\n    padding: 10px;\r\n    color:#fff;\r\n    background:#000;\r\n    button {\r\n\r\n    }\r\n}\r\n</style>"]}]}